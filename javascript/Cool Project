
function Human(name) {
  this.name = name;
}

Human.prototype.feed = function(cat) {
  console.log(this.name + " is feeding " + cat.name);
};


var bob = new Human("Bob");
var tom = new Cat("Tom");

bob.feed(tom);



function Cat(name) {
  this.name = name;
}

Cat.prototype.eat = function(cat) {
  console.log(this.name + " is eating...");
};

Cat.prototype.makeNoise = function(cat) {
  console.log("I'm still hungry, meow");
};

var tom = new Cat("Tom");

tom.eat();
tom.makeNoise();



function Dog(name) {
  this.name = name;
}

Dog.prototype.eat = function(dog) {
  console.log(this.name + " remains silent");
};

var spot = new Dog("Spot");

spot.eat();







